<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
  <key>Label</key>
  <string>com.atlas.git-sync-deferred-restart</string>
  <key>ProgramArguments</key>
  <array>
    <string>/opt/homebrew/bin/python3</string>
    <string>/Users/printer/atlas/tools/system/git_sync_and_restart.py</string>
    <string>--deferred-only</string>
  </array>
  <key>WorkingDirectory</key>
  <string>/Users/printer/atlas</string>
  <key>StartInterval</key>
  <integer>600</integer>
  <key>StandardOutPath</key>
  <string>/Users/printer/atlas/logs/git-sync-deferred.log</string>
  <key>StandardErrorPath</key>
  <string>/Users/printer/atlas/logs/git-sync-deferred.log</string>
</dict>
</plist>
